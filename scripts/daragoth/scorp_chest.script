#include chests/base_treasurechest

{ chest_additems

	callevent add_gold $rand(50,300)
	addstoreitem STORENAME proj_bolt_silver 25 0 0 25
	addstoreitem STORENAME smallarms_royaldagger 1 0
	addstoreitem STORENAME blunt_greatmaul 1 0
	addstoreitem STORENAME axes_poison1 1 0
	
	if ( $rand(1,2) == 1 ) addstoreitem STORENAME mana_protection 1 0
	if ( $rand(1,5) == 1 ) addstoreitem STORENAME armor_helm_knight 1 0
	if ( $rand(1,10) == 1 ) addstoreitem STORENAME mana_demon_blood 1 0
}