{ game_spawn

	name_unique spawner
	fly 1
	invincible 1
	dbg chest_maker spawned
	callevent 0.1 make_chest
}

{ make_chest

	local ROLL $rand(1,6)
	if ( ROLL equals 1 )
	{
		dbg spawning chest_great
		createnpc mines/chest_great $get(ent_me,origin)
	}
	if ( ROLL isnot 1 )
	{
		dbg spawning chest_good
		createnpc mines/chest_good $get(ent_me,origin)
	}
}