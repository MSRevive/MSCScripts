{ ext_ent_list_sort //SCAN_TYPE SCAN_RANGE SCAN_ORIGIN SORT_TYPE:range|hp|maxhp|mp|maxmp
   local SCAN_TYPE PARAM1
   local SCAN_RANGE PARAM2
