//Anti-cheat thing to prevent people farming the buggy horror (name = mask)
{ game_spawn

	//dbg temp ghoul trap triggered
	name_unique map_verify
	name a|Map Verifier
	setmodel none
	race beloved
	setsolid none
	blood none
	width 8
	height 8
	fly 1
	roam 0
	gravity 0
	setvard IMMUNE_PUSH 1
	invincible 1
	hp 1

	callevent 0.1 warp_out
}

{ warp_out
	//make sure I'm out of the way
	setorigin ent_me (20000,-20000,20000)
}