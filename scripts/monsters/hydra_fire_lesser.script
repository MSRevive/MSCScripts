	playsound 0 10 SOUND_ATTACK_HEAD1
}

{ frame_bite2_start
	playsound 0 10 SOUND_ATTACK_HEAD2
}

{ frame_bite3_start
	playsound 0 10 SOUND_ATTACK_HEAD3
